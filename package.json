{
  "name": "socket-chat-example",
  "version": "0.0.1",
  "description": "my first socket.io app",
  "dependencies": {
    "express": "^4.17.1",
    "socket.io": "^4.1.2",
    "sqlite3": "^5.0.2",
    "typeorm": "^0.2.40",
    "ts-node": "^10.4.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.7",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "dev": "NODE_ENV=dev ts-node-dev --transpile-only --ignore-watch node_modules ./src/server.ts",
    "start": "cd build; NODE_ENV=prod node src/server.js",
    "build": "tsc && NODE_ENV=prod node ./node_modules/typeorm/cli.js migration:run",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js"
  },
  "devDependencies": {
  }
}
